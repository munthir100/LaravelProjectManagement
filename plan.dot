technical laravel controllers and methods and validation requests:

account.LoginController
    methods:_
        -showLoginForm()
            view:_
                account.auth.login
        -login()
            validationRequest:_
                account.LoginRequest
        -logout()

account.auth.RegisterController
    methods:_
        -showRegistrationForm()
            view:_
                account.auth.register
        -register()
            validationRequest:_
                account.RegisterRequest
                
account.auth.ForgotPasswordController
    methods:_
        -showForgetPasswordForm()
            view:_
                account.auth.forgetPassword
        -resetPassword()
            validationRequest:_
                account.auth.ResetPasswordRequest

AuthentcatedAccountMiddleware:_
    Account.AccountSettingsController:_
        methods:_
            showSettings()
                view:_
                    account.settings
            updateSettings()
                validationRequest:_
                    UpdateAccountSettingsRequest
